{
	"folders":[
		"config",
		"controllers",
		"tests",
		"vendors",
		"views",
		"views/pages",
		"webroot"
	],
	"files":[
		"config/app.js",
		"config/main.js",
		"config/routes.js",
		"config/settings.js",
		"controllers/page_controller.js",
		"tests/test.js",
		"tests/system_defaults.js",
		"tests/users.js",
		"views/pages/home.html",
		"views/pages/sample.html",
		"index.html"
	],
	"findreplace":{
		"index.html":{
			"find":["Simple UI/X Template","assets/","vendors/bower_components/","/app"],
			"replace":["APP","app/assets/","app/bower_components/","#"]
		},
		"config/app.js":{
			"find":["settings.VIEWS_DIRECTORY+\"\/auth\/login."],
			"replace":["\"app\/views\/auth\/login."]
		},
		"config/main.js":{
			"find":["baseUrl:''","config/settings","config/app"],
			"replace":["baseUrl:'app'","../config/settings","../config/app"]
		},
		"config/settings.js":{
			"find":["controllers","tests"],
			"replace":["../controllers","../tests"]
		}
	}
	
}